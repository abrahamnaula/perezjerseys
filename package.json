{
  "name": "soccer-jersey",
  "version": "1.0.14",
  "description": "Generate soccer jerseys in SVG format",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p .",
    "build:umd": "webpack --mode=production",
    "dev:umd": "webpack serve --mode=development --progress --open",
    "build:deploy": "npm version patch && npm run build-storybook && npm run build:umd && npm run build",
    "doc": "typedoc src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "soccer",
    "shirt",
    "svg",
    "fpl"
  ],
  "author": "nadchif (https://github.com/nadchif)",
  "repository": {
    "type": "git",
    "url": "https://github.com/nadchif/soccer-jersey.git"
  },
  "license": "MIT",
  "files": [
    "lib",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@storybook/addon-links": "9.1.17",
    "@storybook/react-webpack5": "9.1.17",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "babel-loader": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "9.1.17",
    "ts-loader": "^9.5.0",
    "typedoc": "^0.28.16",
    "typescript": "5.1.6",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2",
    "eslint-plugin-storybook": "9.1.17",
    "@storybook/addon-docs": "9.1.17"
  },
  "dependencies": {
    "@svgdotjs/svg.js": "^3.2.0",
    "abab": "^2.0.5"
  }
}
